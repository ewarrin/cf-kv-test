{
    "name": "cf-kv",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "kv:populate": "node scripts/migrate-to-kv.js",
        "kv:status": "curl -X POST $NUXT_PUBLIC_BASE_URL/api/kv/manage -H 'Content-Type: application/json' -d '{\"action\":\"status\"}'",
        "kv:clear": "curl -X POST $NUXT_PUBLIC_BASE_URL/api/kv/manage -H 'Content-Type: application/json' -d '{\"action\":\"clear\"}'"
    },
    "dependencies": {
        "@nuxt/image": "^1.11.0",
        "@nuxt/ui": "^4.0.1",
        "nuxt": "^4.1.3",
        "typescript": "^5.9.3",
        "vue": "^3.5.22",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.21",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.14",
        "wrangler": "^4.43.0"
    }
}
